<?xml version="1.0" encoding="UTF-8"?>
<components>
	<component name="div" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="true" can-be-layer="true"></component>
	<component name="header" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="true" can-be-layer="true"></component>
	<component name="footer" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="true" can-be-layer="true"></component>
	<component name="main" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="true" can-be-layer="true"></component>
	<component name="nav" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="true" can-be-layer="true"></component>
	<component name="h1" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="h2" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="h3" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="h4" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="h5" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="h6" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-contents-direction="true" can-set-scrollable="false" can-be-layer="false"></component>
	<component name="p" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="blockquote" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="span" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="code" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="ul" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="ol" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="li" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="a" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-onclick-event="true">
		<fields>
			<field type="text" name="href" label="href" />
			<field type="text" name="target" label="Target window" />
		</fields>
		<template><![CDATA[<a href="{{ attributes.href }}"{% if attributes.target %} target="{{ attributes.target }}"{% endif %}{% if attributes.class %} class="{{ attributes.class }}"{% endif %}{% if attributes.onclick %} onclick="{{ js_script_prefix }}{{ attributes.onclick }}"{% endif %}>{{ innerHTML | raw }}</a>]]></template>
	</component>
	<component name="button" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true" can-set-onclick-event="true">
		<fields>
			<field type="text" name="type" label="Type" />
			<field type="text" name="value" label="Value" />
		</fields>
		<template><![CDATA[<button type="{{ attributes.type }}" value="{{ attributes.value }}"{% if attributes.class %} class="{{ attributes.class }}"{% endif %}{% if attributes.onclick %} onclick="{{ js_script_prefix }}{{ attributes.onclick }}"{% endif %}>{{ innerHTML | raw }}</button>]]></template>
	</component>
	<component name="img" is-void-element="true" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true">
		<fields>
			<field type="image" name="image" label="Image" />
			<field type="text" name="alt" label="Alt text" />
		</fields>
		<template><![CDATA[{% set image = json_decode(attributes.image) %}
<img src="{{ assets[image.assetId].path }}" width="{{ assets[image.assetId].width }}" height="{{ assets[image.assetId].height }}" alt="{{ attributes.alt }}"{% if attributes.class %} class="{{ attributes.class }}"{% endif %} />]]></template>
	</component>
	<component name="hr" is-void-element="true" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true"></component>
	<component name="br" is-void-element="true" can-set-css="true" can-set-class="true" can-set-width="false" can-set-height="false"></component>
	<component name="form" is-void-element="false" can-set-css="false" can-set-class="false" can-set-onsubmit-event="true">
		<fields>
			<field type="text" name="action" label="Action" />
			<field type="text" name="method" label="method" />
			<field type="text" name="target" label="Target window" />
		</fields>
		<template><![CDATA[<form action="{{ attributes.action }}" method="{{ attributes.method }}"{% if attributes.target %} target="{{ attributes.target }}"{% endif %}{% if attributes.onsubmit %} onsubmit="{{ js_script_prefix }}{{ attributes.onsubmit }}"{% endif %}>{{ innerHTML | raw }}</form>]]></template>
	</component>
	<component name="input" is-void-element="true" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true">
		<fields>
			<field type="text" name="type" label="type" />
			<field type="text" name="name" label="name" />
			<field type="text" name="value" label="value" />
		</fields>
		<template><![CDATA[<input type="{{ attributes.type }}" name="{{ attributes.name }}"{% if attributes.value %} value="{{ attributes.value }}"{% if attributes.class %} class="{{ attributes.class }}"{% endif %} />]]></template>
	</component>
	<component name="textarea" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true">
		<fields>
			<field type="text" name="name" label="name" />
			<field type="text" name="value" label="value" />
		</fields>
		<template><![CDATA[<textarea name="{{ attributes.name }}"{% if attributes.class %} class="{{ attributes.class }}"{% endif %}>{{ attributes.value }}</textarea>]]></template>
	</component>
	<component name="select" is-void-element="false" can-set-css="true" can-set-class="true" can-set-width="true" can-set-height="true">
		<fields>
			<field type="text" name="name" label="name" />
		</fields>
		<template><![CDATA[<select name="{{ attributes.name }}"{% if attributes.class %} class="{{ attributes.class }}"{% endif %}>{{ innerHTML | raw }}</select>]]></template>
	</component>
	<component name="option" is-void-element="false">
		<fields>
			<field type="text" name="value" label="value" />
		</fields>
		<template><![CDATA[<option value="{{ attributes.value }}">{{ innerHTML | raw }}</option>]]></template>
	</component>
</components>
